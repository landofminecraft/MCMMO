{
    "multipart": [
        {   "when": {"OR": [{"north": true}, {"north": false}]},
            "apply": { "model": "mcmmo:green_concrete_structure_wall_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "mcmmo:green_concrete_structure_wall_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "mcmmo:green_concrete_structure_wall_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "mcmmo:green_concrete_structure_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "mcmmo:green_concrete_structure_wall_side", "y": 270, "uvlock": true }
        }
    ]
}
